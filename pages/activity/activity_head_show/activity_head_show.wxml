<!--pages/activity/activity_head_show/activity_head_show.wxml-->
<view class='contina'>
  <view class='activityLine'>活动编号：
    <text class='activityName'>{{activityData.activity_code?activityData.activity_code:''}}</text>
  </view>
  <view class='activityLine'>活动名称：
    <text class='activityName'>{{activityData.name?activityData.name:''}}</text>
  </view>
  <view class='flexRow activityBa '>
    <view>分享次数</view>
    <view>邀约次数</view>
  </view>
  <view class='flexRow activityBa1'>
    <view>
      <text class='textValue'> {{activityData.shareNum?activityData.shareNum:0}}</text>
      <text> 次</text> </view>
    <view>
      <text class='textValue'> {{activityData.inviteNum?activityData.inviteNum:0}}</text>
      <text> 位</text>
    </view>
  </view>
  <view class='flexRow activityBa '>
    <view>签到客户</view>
    <view>售卡量</view>
  </view>
  <view class='flexRow activityBa1'>
    <view>
      <text class='textValue'> {{activityData.signNum?activityData.signNum:0}}</text>
      <text> 位</text> </view>
    <view>
      <text class='textValue'> {{activityData.cardNum?activityData.cardNum:0}}</text>
      <text> 张</text>
    </view>
  </view>
  <view class='flexRow activityBa '>
    <view>已付金额</view>
    <view>订单数量</view>
  </view>
  <view class='flexRow activityBa1'>
    <view>
      <text class='textValue'> {{activityData.deposit_price?activityData.deposit_price:0}}</text>
      <text> 元</text> </view>
    <view>
      <text class='textValue'> {{activityData.orderNum?activityData.orderNum:0}}</text>
      <text> 单</text>
    </view>
  </view>
</view>