  height:150rpx;
  border-radius:50%;
}
.activityGift{
  width:430rpx;
  height:auto;
}
.standard{
  height:120rpx;
}
.standard2{
  height:60rpx;
}
.standard view{
  font-size:28rpx;
  color:#fff;
  line-height:140rpx;
}
.standard .names{
  font-size:34rpx;
  color:#fff;
}
.exchangeCode{
  width:250rpx;
  height:50rpx;
  line-height:50rpx;
  color:#fff;
  font-size:20rpx;
  background:rgba(255, 255, 255, 0.3);
  text-align:center;
}
.states{
  color:#fff;
  font-size:24rpx;
  text-align:center;
  width:100rpx;
  height:50rpx;
  line-height:50rpx;
  margin-left:40rpx;
  border:2rpx dashed #fff;
  box-sizing:border-box;
}
