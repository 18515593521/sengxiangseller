  box-sizing:border-box;
}
.couponList{
  width:750rpx;
  height:auto;
}
.couponItem{
  position:relative;
  width:100%;
  height:200rpx;
}
.bgCoupon{
  width:100%;
  height:100%;
}
.bgCoupons{
  width:200rpx;
  height:146.6rpx;
  position:absolute;
  z-index:1000000;
  margin:auto;
  right:131rpx;
  top:32rpx;

}
.activityDetail{
  position:absolute;
  width:100%;
  margin:auto;
  height:100%;
  top:0;
  left:0;
  bottom:0;
  right:0;
}
.gift{
  padding-left:20rpx;
  box-sizing:border-box;
  font-size:26rpx;
  color:#999;
}
.headImg{
  width:320rpx;
  height:100%;
