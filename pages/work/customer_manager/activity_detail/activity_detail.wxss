.activityName{
  width:750rpx;
  height:60rpx;
  line-height:60rpx;
  border-top:2rpx solid #eee;
   border-bottom:2rpx solid #eee;
  font-size:28rpx;
  color:#999;
  padding-left:20rpx;
  box-sizing:border-box;
}
.images{
  width:750rpx;
  height:466rpx;
}
.images image{
  width:100%;
  height:100%;
}
.activityInfo{
  width:750rpx;
  height:120rpx;
  padding:0 20rpx;
  box-sizing:border-box;
}
.noDatay{
  width:750rpx;
  text-align:center;
  font-size:26rpx;
  margin-top:20rpx;
}

.activityItem view{
  color:#999;
  font-size:26rpx;
  line-height:60rpx;
  width:50%;
}
.title{
