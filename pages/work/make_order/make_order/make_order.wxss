  background:#44c0c1;
  line-height:100rpx;
  font-size:28rpx;
  color:#fff;
}

.sale,.totalSalePrice{
  font-size:26rpx;
  line-height:60rpx;
  float:right;
  width:300rpx;
  text-align:right;
}
.totalSalePrice{
  text-align:right;
  margin-right:20rpx;
}
.remark{
  overflow:hidden;
  border-top:10rpx solid #eee;
  width:750rpx;
  height:60rpx;
  border-bottom:2rpx solid #eee;
}
.wenhao{
  width:30rpx;
  height:30rpx;
  float:left;
  margin:16rpx;
}
.remark view{
width:200rpx;
font-size:26rpx;
float:left;
line-height:60rpx;
}
.remarkText{
  font-size:24rpx;
  color:#333;
  width:750rpx;
  overflow:hidden;
  height:150rpx;
  float:left;
}
.explain{
  width:750rpx;
  height:auto;
  overflow:hidden;
  padding:10rpx 20rpx;
  font-size:24rpx;
  border-bottom:2rpx solid #eee;
  z-index:-10000;
  box-sizing:border-box;
}
.submit{
  width:750rpx;
  height:100rpx;
  background:#44c0c1;
  text-align:center;
  font-size:30rpx;
  color:#fff;
  line-height:100rpx;
}

/*省市区选择器*/
.pickerViewContainer{
  position: fixed;
  bottom: 0;
  background-color: #F5F5F5;
  opacity: 1;
  z-index:10000;
  border-top: 1rpx solid #B2B2B2;
}
.pickerViewOperate{
  padding: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  color: #4ABDC2;
}
.pickerView{
  width: 750rpx;
  height: 400rpx;
}
.pickerColumn{
  line-height: 50px;
  text-align: center;
  font-size: 28rpx;
}