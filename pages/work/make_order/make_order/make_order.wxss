  margin:auto;
}
.pop_up_box{
  width:750rpx;
  height:750rpx;
  background:#fff;
  position:absolute;
  margin:auto;
  bottom:0;
  left:0;
  right:0;
  z-index:10000;
}
.selectTile{
  width:100%;
  line-height:60rpx;
  border-bottom:2rpx solid #eee;
  text-align:center;
  font-size:30rpx;
}
.selectItem{
  width:100%;
  height:80rpx;
  line-height:80rpx;
  border-bottom:2rpx solid #eee;
  padding:0 20rpx;
