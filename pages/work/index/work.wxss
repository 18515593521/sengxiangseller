/* pages/work/index/work.wxss */
 /*模态框*/
 @import "/pages/template/template.wxss"; 
 
.task_list{
  width:750rpx;
  background:#f9f9f9;
  padding:20rpx;
  box-sizing:border-box;
  height:auto;
  overflow:hidden;
}
.task_title{
  text-align:center;
  font-size:32rpx;
}
.task_box{
  flex-flow:row nowrap;
  display:flex;
  margin-top:20rpx;
  border-radius:10px;
}
.yesterday_state{
  width:750rpx;
}
.task_content{
  border:2rpx solid #a2dadd;
  width:75%;
  height:auto;
  overflow:hidden;
  display:flex;
  padding:10rpx;
  box-sizing:border-box;
  background:#fff;
  border-top-left-radius:10px;
   border-bottom-left-radius:10px;
}
.task_content_next{
  flex-flow:column nowrap;
}
.sell_deail{
  flex-grow:1;
  flex-flow:column nowrap;
  padding:20rpx 0;
 box-sizing:border-box;
 border-bottom:2rpx solid #f7f7f7;
}
.sell_deail_icon{
  background:#a4dee0;
  padding:40rpx 15rpx;
  box-sizing:border-box;
  text-align:center;
  width:25%;
  align-items:center;
    border-top-right-radius:10px;
   border-bottom-right-radius:10px;
}
.model{
  text-align:center;
}
.activity_icons{
  width:40rpx;
  height:40rpx;
}
.activity_titles{
  color:#fff;
  font-size:30rpx;
}
.bargain_money{
  flex-flow:column nowrap;
}
.task_all_num{
  color:#41c1c0;
  font-size:40rpx;
}
.task_nums_all{
  font-size:30rpx;
}
.finsh_num{
  font-size:20rpx;
}
.color_gray{
  color:#666;
}
.percentage{
  display:flex;
  justify-content:center;
}
.tast_activity_title{
  font-size:26rpx;
  color:#666;
  white-space:wrap;
  padding:5px 0 0 22rpx;
  box-sizing:border-box;

}
/*标签*/
.icon_content{
  width:750rpx;
  display:flex;
  flex-flow:row wrap;
  overflow:hidden;
  border-top:2rpx solid #eee;
  margin: 0 15rpx;
}

.item-icon{
  width:180rpx;
  height:200rpx;
  border-right:2rpx solid #f9f9f9;
  border-bottom:2rpx solid #f9f9f9;
  display:flex;
  flex-flow:column nowrap;
  justify-content:center;
  padding:15rpx;
  box-sizing:border-box;
  
}
.item-icon image{
  width:60rpx;
  height:60rpx;
  margin:0 auto;
}
.icon_content .item-icon:nth-child(3) image{
  width:60rpx;
  height:65rpx;
}
.icon_content .item-icon:nth-child(4) image {
  width: 68rpx;
  height:60rpx;
}
.icon_content .item-icon:nth-child(2) image,.icon_content .item-icon:nth-child(5) image{
  width:60rpx;
  height:63rpx;
}
.title_icons{
  text-align:center;
  font-size:28rpx;
  color:#666;
  margin-top:20rpx;
}
/*今日数据*/
.data_title{
  background:#f9f9f9;
  padding:20rpx;
  width:750rpx;
  height:80rpx;
  text-align:center;
  color:#333;
  font-size:32rpx;
  box-sizing:border-box;
}
.datas_list{
  width:750rpx;
  height:auto;
  display:flex;
  flex-flow:column wrap;
}
.datas_list_item{
  height:80rpx;
  box-sizing:border-box;
  display:flex;
  flex-flow:row nowrap;
  border-bottom:2rpx solid #f9f9f9;
  align-items:center;
  padding:0 20rpx;
}
.right_icon,.data_icon{
  width:30rpx;
  height:30rpx;
}
.data_name,.data_num{
  flex-grow:4;
  font-size:30rpx;
  color:#666;
  text-overflow:ellipsis;
  align-items:center;
  line-height:80rpx;
}
.report_phone{
  width:650rpx;
  height:80rpx;
  margin:auto;
  text-align:center;
  color:#fff;
  font-size:30rpx;
  background:#4abdc2;
  line-height:80rpx;
  border-radius:5px;
}
.report_user{
  background:rgba(183, 183, 183, 0.5);
  position:fixed;
  top:0;
  bottom:0;
  margin:auto;
  left:0;
  right:0;
}
.report_box{
  position:absolute;
  margin:auto;
  top:0;
  bottom:0;
  right:0;
  left:0;
  width:600rpx;
  height:400rpx;
  border:2rpx solid #4abdc2;
  border-radius:5px;
  display:flex;
  flex-flow:column nowrap;
  background:#fff;
}
.user_info_box{
  padding:30rpx;
  box-sizing:border-box;
  display:flex;
  flex-flow:row nowrap;
}
.operation_box{
  display:flex;
  flex-flow:row nowrap;
  border-top:2rpx solid #4abdc2;
  height:80rpx;
}
.user_info_name{
  font-size:30rpx;
  width:150rpx;
  height:60rpx;
  line-height:60rpx;
  margin-right:20rpx;
}
.user_name{
  background:#fff;
  color:#666;
  font-size:30rpx;
  border:2rpx solid #4abdc2;
  border-radius:5px;
  height:60rpx;
  width:450rpx;
  line-height:60rpx;
}
.cancel,.sure{
  font-size:30rpx;
  text-align:center;
  line-height:80rpx;
  width:50%;
}
.general_state{
  display:flex;
  flex-flow:row nowrap;
  line-height:50rpx;
  padding:20rpx;
  box-sizing:border-box;
}
.notice_img{
  width:30rpx;
  height:30rpx;
  margin-left:10rpx;
  margin:auto 5px;

}
.upload_time{
  color:#999;
  font-size:24rpx;
  text-align:right;
  width:600rpx;
}
.yesterday_list{
  display:flex;
  flex-flow:row nowrap;
}
.userNum{
  color:#999;
  font-size:26rpx;
  text-align:center;
}
.userNumShow{
  font-size:36rpx;
  text-align:center;
  
}
.user_num{
  border:2rpx solid #999;
  margin:0 20rpx 20rpx;
  flex-grow:1;
  padding:20rpx;
  box-sizing:border-box;
}

.collect_num{
  color:red;
  font-size:24rpx;
  text-align: center;
  
}

.newcardBox{
  position:fixed;
  z-index: 10000;
  top:0;
  left:0;
  right:0;
  bottom:0;
  background:rgba(0, 0, 0, 0.5);
  margin:auto;
}

.commissionImage {
  position: absolute;
  top: 50vh;
  margin-top: -350rpx;

}

.newcardBox .delete{
  display: flex;
  flex-direction: row-reverse; 
}
.newcardBox .delete image{
  width: 30rpx;
  height: 30rpx;
  display: block;
  margin: 20rpx;
}
.commissionImage  .image{
    margin-left: 125rpx;
    width: 500rpx;
    height: 700rpx;
}