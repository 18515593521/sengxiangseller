.integral_box{
  width:750rpx;
  height:280rpx;
  background:#0074c1;
}
.score_title{
  color:#fff;
  font-size:26rpx;
  height:60rpx;
}
.score_code{
  width:100%;
  height:220rpx;
  line-height:220rpx;
  text-align:center;
  color:#fff;
  font-size:120rpx;
}
.score_sing{
   color:#fff;
  font-size:32rpx;
  height:60rpx;
  text-align:center;
  position:fixed;
  margin:auto;
  right:40rpx;
  top:30rpx;
  z-index: 10000;
}
.score_box_list{
  width:750rpx;
  height:auto;
}
.images{
  width:50rpx;
  height:60rpx;
  display:flex;
}
.score_item{
  padding:0 10rpx;
  box-sizing:border-box;
  width:100%;
  height:80rpx;
  border-bottom:2rpx solid #eee;
  align-items:center;
}
.title{
  width:700rpx;
  font-size:28rpx;
  line-height:60rpx;
}
.images image{
  width:35rpx;
  height:35rpx;
  align-items:center;
  margin:auto;
}

/*二维码*/
.imagePannel{
  position:fixed;
  width: 100vw;
  height: 100wh;
  bottom: 0;
  right: 0;
  z-index: 210;
}
.imagePannel .code{
  width: 100%;
  height: 100%;
}

.imagePannel .code image{
 width: 100%;
  height: 100%;
  display: block;
}
.textW{
  margin: 0 auto;
  font-size:32rpx; 
  margin-top: 20rpx;
}
.textM{
   margin: 0 auto;
}
.textM image{
   margin: 0 auto;
  height: 200rpx;
  width: 200rpx;
}
.signText{
  position: absolute;
  top: 33vh;
  left: 34vw;
  font-size: 28rpx;

}

