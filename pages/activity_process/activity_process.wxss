.infoBasice{
  width:750rpx;
  font-size:28rpx;
  line-height:60rpx;
  padding:0 20rpx;
  box-sizing:border-box;
  border-bottom:20rpx solid #eee;
}
.activityImg{
  width:450rpx;
  height:240rpx;
}
.destroy{
  width:220rpx;
  height:155rpx;
  position:absolute;
  margin:auto;
  z-index:1000000000;
  right:105rpx;
  bottom:22rpx;
}
.sureBuy,.sign,.makeOrder{
  width:180rpx;
  height:60rpx;
  display:block;
  border-radius:20rpx;
  background:#0074c1;
  color:#fff;
  font-size:26rpx;
  text-align:center;
  line-height:60rpx;
  margin-left:70rpx;
}
.sign{
  margin-left:230rpx;
}
.select_list{
  width:200rpx;
  border:2rpx solid #eee;
  border-radius:10rpx;
  box-sizing:border-box;
  height:100%;
  position:relative;
  font-size:28rpx;
  text-align:center;
  line-height:60rpx;
}
.select_list .from_text{
  width:100%;
  float:left;
  padding:0 10rpx;
  box-sizing:border-box;
  text-align:center;
  color:#eee;
  position:absolute;
}
.from_text view{
  font-size:24rpx;
  color:#666;
  line-height:60rpx;
  margin-right:30rpx;
}
.pullDown{
  width:30rpx;
  height:30rpx;
  float:right;
  margin:15rpx 10rpx;
}
.store,.edit,.print{
  width:30rpx;
  height:30rpx;
}
.activityTitle{
  line-height:60rpx;
  padding:0 20rpx;
  box-sizing:border-box;
  font-size:28rpx;
}
.couponList{
  width:750rpx;
  height:auto;
}
.couponItem{
  position:relative;
  width:100%;
  height:200rpx;
}
.bgCoupon{
  width:100%;
  height:100%;
}
.activityDetail{
  position:absolute;
  width:100%;
  margin:auto;
  height:100%;
  top:0;
  left:0;
  bottom:0;
  right:0;
}
.gift{
  padding-left:130rpx;
  box-sizing:border-box;
  font-size:26rpx;
  color:#999;
  line-height:60rpx;
}
.headImg{
  width:320rpx;
  height:100%;
  display:flex;
}
.headImg image{
  align-items:center;
  margin:auto;
  width:150rpx;
  height:140rpx;
  border-radius:50%;
}
.activityGift{
  width:430rpx;
  height:auto;
}
.standard{
  height:120rpx;
}
.standard2{
  height:60rpx;
}
.standard view{
  font-size:28rpx;
  color:#fff;
  line-height:140rpx;
}
.standard .names{
  font-size:34rpx;
  color:#fff;
}
.exchangeCode{
  width:250rpx;
  height:50rpx;
  line-height:50rpx;
  color:#fff;
  font-size:20rpx;
  background:rgba(255, 255, 255, 0.3);
  text-align:center;
}
.states{
  color:#fff;
  font-size:24rpx;
  text-align:center;
  width:100rpx;
  height:50rpx;
  line-height:50rpx;
  margin-left:40rpx;
  border:2rpx dashed #fff;
  box-sizing:border-box;
}
.clickMake{
  width:180rpx;
  height:60rpx;
  line-height:60rpx;
  text-align:center;
  background:#fff;
  font-size:28rpx;
  display:block;
  border-radius:10rpx;
}
.canMake{
  border:2rpx solid #0074c1;
  color:#0074c1;
}
.noMake{
  border-radius:2rpx solid #999;
  color:#999;
}
.makeOrder{
  float:right;
  margin:20rpx;
}
.report_user{
  background:rgba(183, 183, 183, 0.5);
  position:fixed;
  top:0;
  bottom:0;
  margin:auto;
  left:0;
  right:0;
}
.report_box{
  position:absolute;
  margin:auto;
  top:0;
  bottom:0;
  right:0;
  left:0;
  width:600rpx;
  height:280rpx;
  border:2rpx solid #0074c1;
  border-radius:5px;
  display:flex;
  flex-flow:column nowrap;
  background:#fff;
}
.user_info_box{
  padding:30rpx;
  box-sizing:border-box;
  display:flex;
  flex-flow:row nowrap;
}
.operation_box{
  display:flex;
  flex-flow:row nowrap;
  border-top:2rpx solid #0074c1;
  height:80rpx;
}
.user_info_name{
  font-size:30rpx;
  width:150rpx;
  height:60rpx;
  line-height:60rpx;
  margin-right:20rpx;
}
.user_name{
  background:#fff;
  color:#666;
  font-size:30rpx;
  border:2rpx solid #0074c1;
  border-radius:5px;
  height:60rpx;
  width:450rpx;
  line-height:60rpx;
}
.cancel,.sure{
  font-size:30rpx;
  text-align:center;
  line-height:80rpx;
  width:50%;
}
