.phone{
  font-size:28rpx;
  text-align: center;
  margin-bottom:30rpx; 
}
.newPhone{
  width:100%;
  height:80rpx;
  border:2rpx solid #50bdc4;
  display:block;
  font-size:28rpx;
  line-height:80rpx;
  padding-left:20rpx;
  box-sizing:border-box;
}
.pullCode{
  width:70%;
  border:#fff;
  height:80rpx;
  line-height:80rpx;
  padding-left:20rpx;
  box-sizing:border-box;
  font-size:28rpx;
  border:2rpx solid #50bdc4;
}
.clickCode{
  color:#fff;
  width:30%;
  font-size:26rpx;
  background:#50bdc4;
  height:80rpx;
  text-align:center;
  line-height:80rpx;
}
.sure{
  width:200rpx;
  height:80rpx;
  background:#50bdc4;
  color:#fff;
  text-align:center;
  line-height:80rpx;
  font-size:26rpx;
  margin:30rpx auto;
}