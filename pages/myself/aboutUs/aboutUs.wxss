.images{
  display:flex;
  align-items:center;
  width:750rpx;
  height:300rpx;
  background:#eee;
}
.images image{
  width:200rpx;
  height:150rpx;
  margin:auto;
}
.item{
  width:750rpx;
  display:flex;
  align-items:center;
  height:100rpx;
  font-size:28rpx;
  border-bottom:2rpx solid #eee;
}
.imgs image{
  width:40rpx;
  height:40rpx;
  float:right;
}
.title,.imgs{
  width:50%;
  overflow:hidden;
}
.phone{
  font-size:28rpx;
  text-align:right;
}